# Peptiditor script for deriving active-state conformational changes from bovine rhodopsin.
#

# Load the active protein.
LOAD "pdbs/ADRB2/4ldo.active.pdb"

DELETE 1 1028

RENUMBER 1029 9999 29

ECHO %SEQLEN
ECHO $SEQUENCE
ECHO ""

REGION TMR1 35 58
REGION TMR2 72 95
REGION TMR3 107 129
REGION TMR4 151 174
REGION TMR5 197 220
REGION TMR6 275 298
REGION TMR7 306 329

UPRIGHT

SAVE "pdbs/ADRB2/ADRB2.active.pdb"

LET %termin = 4

# Obtain the active helix termini.

LET %i = 1
_loop:

LET %j = %TMR%i.s
LET %n = 0
_loopn:
LET @an[%i]  = %j
LET %j ++ 
LET %n ++
IF %n < %termin GOTO _loopn

LET @an[%i] /= %n


LET %j = %TMR%i.e
LET %n = 0
_loopn1:
LET @ac[%i]  = %j
LET %j -- 
LET %n ++
IF %n < %termin GOTO _loopn1

LET @ac[%i] /= %n


LET %i ++

IF %i <= 7 GOTO _loop


# Load the inactive protein.
LOAD "pdbs/ADRB2/2rh1.inactive.pdb"

ECHO %SEQLEN
ECHO $SEQUENCE
ECHO ""

REGION TMR1 35 58
REGION TMR2 72 95
REGION TMR3 107 129
REGION TMR4 151 174
REGION TMR5 197 220
REGION TMR6 275 298
REGION TMR7 306 329

UPRIGHT

SAVE "pdbs/ADRB2/ADRB2.inactive.pdb"


# Obtain the inactive termini.

LET %i = 1
_loop1:

LET %j = %TMR%i.s
LET %n = 0
_loopn2:
LET @in[%i]  = %j
LET %j ++ 
LET %n ++
IF %n < %termin GOTO _loopn2

LET @in[%i] /= %n

# ECHO "TMR" %i ": Average of four residues starting at " %TMR%i.s " = " @in[%i]

LET %j = %TMR%i.e
LET %n = 0
_loopn3:
LET @ic[%i]  = %j
LET %j -- 
LET %n ++
IF %n < %termin GOTO _loopn3

LET @ic[%i] /= %n

# ECHO "Got inactive start/end averages for TMR" %i "..."

LET %i ++
IF %i <= 7 GOTO _loop1


# Center the data by subtracting the average.

LET @avgdeltae = 0
LET @avgdeltac = 0

LET %i = 1
_loop3:

LET %odd = %i & 1
LET %even = 1 - %odd

LET @delta_n = @an[%i] - @in[%i]
LET @delta_c = @ac[%i] - @ic[%i]

IF %odd THEN LET @avgdeltae += @delta_n
IF %even THEN IF %i != 6 THEN LET @avgdeltac += @delta_n
IF %odd THEN IF %i != 5 THEN LET @avgdeltac += @delta_c
IF %even THEN LET @avgdeltae += @delta_c

LET %i ++
IF %i <= 7 GOTO _loop3

LET @avgdeltae /= 7
LET @avgdeltac /= 5

LET %i = 1
_loop4:

LET %odd = %i & 1

IF %odd THEN LET @an[%i] -= @avgdeltae
ELSE LET @an[%i] -= @avgdeltac
IF %odd THEN LET @ac[%i] -= @avgdeltac
ELSE LET @ac[%i] -= @avgdeltae

LET %i ++
IF %i <= 7 GOTO _loop4


# Output the deltas.

ECHO "# Activation matrix from ADRB2.pepd:"
ECHO "$matrix ="
ECHO "["

LET %i = 1
_loop2:

LET @delta_n = @an[%i] - @in[%i]
LET @delta_c = @ac[%i] - @ic[%i]

ECHO "	'TMR" %i "' => [ " @delta_n.x ", " @delta_n.y ", " @delta_n.z ", " @delta_c.x ", " @delta_c.y ", " @delta_c.z " ],"

LET %i ++

IF %i <= 7 GOTO _loop2
ECHO "];"

# DUMP



