PROT pdbs/OR7/OR7D4.upright.pdb
LIG sdf/androstenone.sdf

CEN RES 259 252 109 108

SIZE 6.0 7.5 5.5

EXCL 1 57		# Head, TMR1, and CYT1.
EXCL 164 196	# EXR2 between TMR4 and TMR5.

POSE 5

FLEX 1

ITERS 50

OUT output/OR7D4_androstenone.test.dock

